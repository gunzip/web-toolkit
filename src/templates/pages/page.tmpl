{% render '@header' %}

<div id="main">

  <div class="u-layout-wide u-layoutCenter u-text-r-xxl u-margin-r-bottom u-layout-r-withGutter">
    {% render '@news' %}
  </div>

  <div class="u-layout-wide u-layoutCenter u-padding-r-bottom u-margin-r-bottom u-layout-r-withGutter">
    {% render '@entrypoint' %}
  </div>

  <div class="u-background-grey-20 u-text-xxl u-padding-r-top u-padding-r-bottom">
    <div class="u-layout-wide u-layoutCenter u-layout-r-withGutter">
      {% render '@layout--grid' %}
    </div>
  </div>

  <div class="u-background-grey-20 u-background-teal-50 u-color-black u-text-xxl u-padding-r-top u-padding-r-bottom">
    <div class="u-layout-wide u-layoutCenter u-layout-r-withGutter">
      {% render '@utilities' %}
    </div>
  </div>

  <div class="u-background-grey-80">
    <div class="u-layout-wide u-layoutCenter u-layout-r-withGutter">
    {% render '@footer' %}
    </div>
  </div>

  {% render '@scrolltop' %}
  {% render '@cookiebar' %}

</div>
